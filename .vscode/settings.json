{
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "files.trimTrailingWhitespace": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.formatOnSave": true,
  "search.exclude": {
    "manifest_tables": true,
    "manifests": true
  },
  "cSpell.language": "en",
  "cSpell.enabledLanguageIds": ["jsonc", "json", "markdown", "typescript", "javascript"],
  "cSpell.allowCompoundWords": true,
  "cSpell.ignorePaths": [
    "package.json",
    ".github",
    ".vscode",
    "built",
    "manifest_tables",
    "manifests",
    "node_modules",
    "output/d2-font-glyphs.ts",
    "output/missing-source-info.ts",
    ".eslintrc",
    "yarn.lock",
  ],
  "cSpell.words": [
    "Acrius",
    "Arach",
    "Árma",
    "Bergusian",
    "calus",
    "Caluseum",
    "cayde",
